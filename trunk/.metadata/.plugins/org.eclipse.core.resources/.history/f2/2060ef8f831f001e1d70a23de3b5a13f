package spectrum;

import java.awt.Graphics;
import java.awt.Image;
import java.awt.image.ImageObserver;

public class Actor 
{
	Sprite spriteSheet;
	
	public Actor(Sprite)
	{
		this.spriteSheet = sprite;
	}
	
	public void Update()
	{
		spriteSheet.Update();
	}
	
	public void Draw(Graphics g, ImageObserver observer, int x, int y)
	{
		spriteSheet.Draw(g, observer, x, y);
	}
}